<h1>My Goals</h1>
<% @mygoals.each do |mygoal| %>
  <h2>MY GOAL: <%= mygoal.name %></h2>
  <h2>99 days to go</h2>
  <div class="progress">
    <div class="progress-bar" role="progressbar" aria-valuenow="1" aria-valuemin="0" aria-valuemax="100" style="width: 1%;">
      <span class="sr-only">1% Complete</span>
    </div>
  </div>
<% end %>

<%= link_to 'NEW GOAL', new_mygoal_path %>