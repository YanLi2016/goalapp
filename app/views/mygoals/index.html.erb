<h1>Listing mygoals</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Start</th>
      <th>Finish</th>
      <th>Days</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @mygoals.each do |mygoal| %>
      <tr>
        <td><%= mygoal.name %></td>
        <td><%= mygoal.start %></td>
        <td><%= mygoal.finish %></td>
        <td><%= mygoal.days %></td>
        <td><%= link_to 'Show', mygoal %></td>
        <td><%= link_to 'Edit', edit_mygoal_path(mygoal) %></td>
        <td><%= link_to 'Destroy', mygoal, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Mygoal', new_mygoal_path %>
