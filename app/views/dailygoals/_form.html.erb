<%= simple_form_for(@dailygoal, :url => dailygoal_path(:id => @dailygoal.id), :method => :patch) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :description %>
    <%= f.input :done %>
    
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
